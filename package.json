{
  "name": "cryptohelp",
  "version": "1.0.0",
  "private": true,
  "description": "Web3 Hackathon & Grants Aggregation Platform",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "dev:web": "turbo dev --filter=web",
    "dev:worker": "turbo dev --filter=worker",
    "build:web": "turbo build --filter=web",
    "build:worker": "turbo build --filter=worker",
    "scrape": "pnpm --filter=worker scrape:all",
    "scrape:hackathons": "pnpm --filter=worker scrape:ethglobal && pnpm --filter=worker scrape:devfolio",
    "scrape:grants": "pnpm --filter=worker scrape:grants"
  },
  "keywords": ["web3", "hackathon", "grants", "blockchain"],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.28.0",
  "devDependencies": {
    "turbo": "^2.3.3",
    "typescript": "^5.7.3"
  }
}
